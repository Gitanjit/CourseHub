<%- include("partials/header") %>
    <h1>Welcome to Coursehub</h1>
    <h2><%= Subject.name %></h2>
    <p><%= Subject.year %></p>
    <p><%= Subject.semester %> semester</p>
    <p><%= Subject.branch %></p>
    <p><%= Subject._id %></p>
    <ul>
        <% Resources.forEach(resource => { %>
            <li> <%= resource.name %> </li>
        <% }) %>
    </ul>
    <form class="" action="/year/<% Subject.year %>/semester/<% Subject.semester %>/branch/<% Subject.branch %>/subject/<% subject_id %>/delete" method="post">
        <input type="hidden" name="_method" value="delete"/>
        <button type="submit" name="button">Delete</button>
    </form>
<%- include("partials/footer") %>
